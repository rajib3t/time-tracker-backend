    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/http-status": "^0.2.30",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/mysql": "^2.15.26",
    "@types/node": "^22.13.10",
    "@types/pg": "^8.11.11",
    "@types/sequelize": "^4.28.20",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
