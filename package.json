{
  "name": "my-endpoint",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "production": "tsc && node dist/server.js",
    "migrate:create": "npx sequelize-cli migration:generate --name",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed:create": "npx sequelize-cli seed:generate --name",
    "seed:all": "npx sequelize-cli db:seed:all",
    "model:create": "npx sequelize-cli model:generate --name"
  },
  "author": "",
  "license": "ISC",
  "description": "",
